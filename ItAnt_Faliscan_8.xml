                            <tei:persName>
                                <tei:name type="praenomen">Uolta</tei:name>
                                <tei:name type="gentilicium">Neroni</tei:name>
                            </tei:persName>
                        </tei:person>
                        <tei:person xml:id="p2" xml:lang="xfa-Ital-x-xfafal">
                            <tei:persName>
                                <tei:name type="praenomen">Ca(ui)</tei:name>
                            </tei:persName>
                        </tei:person>
                    </tei:listPerson>
            </tei:div>
